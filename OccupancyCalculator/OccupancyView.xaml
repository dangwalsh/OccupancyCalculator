<Window x:Class="Gensler.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Occupancy" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid>
        <GroupBox Header="Calculations" Margin="10,10,10,37">
            <Grid Margin="0,0,0,0">
                <DataGrid x:Name="OccupancyGrid" Margin="10,10,8,10"/>
            </Grid>
        </GroupBox>
        <Button x:Name="BtnCancel" Content="Cancel" Margin="0,0,10,10" IsCancel="True" HorizontalAlignment="Right" Width="75" Height="22" VerticalAlignment="Bottom"/>
        <Button x:Name="BtnOk" Content="Ok" Margin="0,0,90,10" IsDefault="True" Click="BtnOk_Click" Height="22" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75"/>
    </Grid>
</Window>
